{
  "name": "heapq",
  "version": "0.1.0",
  "description": "A Heap Queue Implemented in TypeScript",
  "main": "dist-cjs/index.js",
  "module": "dist-esm/index.js",
  "types": "dist-cjs/index.d.ts",
  "author": "koralle",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "npm-run-all -s build:cjs build:esm",
    "build:cjs": "tsc --project . --module commonjs --outDir ./dist-cjs",
    "build:esm": "tsc --project . --module es2015 --outDir ./dist-esm",
    "lint": "eslint --ext={ts,tsx} \"src/**\"",
    "lint:debug": "eslint --debug --ext={ts,tsx} src",
    "lint:fix": "eslint --fix . --ext={ts,tsx} src",
    "fmt": "prettier src",
    "fmt:fix": "yarn fmt -w",
    "nibble": "eslint-nibble src/**",
    "test": "jest",
    "test:coverage": "jest test --coverage"
  },
  "keywords": [
    "typescript",
    "heap",
    "queue",
    "heapq",
    "priority",
    "ts"
  ],
  "bugs": {
    "url": "https://github.com/koralle/heapq/issues"
  },
  "homepage": "https://github.com/koralle/heapq",
  "devDependencies": {
    "@types/jest": "^27.0.3",
    "@types/node": "^16.11.11",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "eslint": "^8.4.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-nibble": "^8.0.0",
    "eslint-plugin-jest": "^25.3.0",
    "jest": "^27.4.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.0",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.2"
  }
}
